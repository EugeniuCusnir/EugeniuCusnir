package ru.eugen.tema.pe.acasa.tema.pe{

    public static class  TemaPentruAcasa {

        int a = 4;
        int b = 5;
        int c = 8;
        int d = 2;
        float div = (float) c / d;
        float sum = (float) b + div;
        float mul = (float) a * sum;
   System.out.println("Your result is:"+mul);


        void printResult(5,6,8,2) {


            static void printResult ( float a, float b, float c, float d){
                float result = a * (b + (c / d));
                System.out.println("Your result is: " + result);
            }
        }
    }

